
<%= render 'stores/sideMenu' %>
<div class="store-content">
  <%# <h1 style="margin-bottom: 50px">List Of orders</h1> %>
    <table class="orders-table">
        <tr class="header">
            <th>Customer Name</th>
            <th>Product code</th>
              <th>Product Name</th>
                <th>Shipping address</th>
                  <th>Price</th>
           
        </tr>

<%@orders.each do |order|%>

<% if order.product.store.id == @store.id && order.status == "delivered"%>

  
        <tr>
            <td><%= order.order.user.name%></td>
            <td><%= order.product.id %></td>
             <td><%= order.product.name %></td>
              <td><%= order.order.address %></td>
               <td><%= order.product.price %></td>
  
        </tr>

            <%end%>
    <%end%>
    </table>
    </div>
 
