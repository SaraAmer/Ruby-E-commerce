<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<table>
  <tr>
    <th>name</th>
    <th>category</th>
    <th>brand</th>
    <th colspan="2"></th>
  </tr>
 
  <% @products .each do |product| %>
    <tr>
    <td><%= product.name %></td>
    <td><%= product.categoryname %></td>
    <td><%= product.brandname %></td>

    </tr>
  <% end %>
</table>